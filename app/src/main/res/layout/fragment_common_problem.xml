<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical"
    android:padding="@dimen/fragment_content_padding">

    <include layout="@layout/content_title" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/button_background"
            android:gravity="center_horizontal"
            android:focusable="true"
            android:orientation="vertical"
            android:padding="@dimen/fragment_content_padding">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/fragment_card_margin"
                    app:cardCornerRadius="@dimen/fragment_card_radius"
                    app:cardElevation="@dimen/fragment_card_elevation">

                    <LinearLayout
                        android:id="@+id/ll_language_modify"
                        android:layout_width="400dp"
                        android:layout_height="350dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40px"
                            android:layout_marginTop="40px"
                            android:text="@string/help_language_title"
                            android:textColor="@android:color/holo_green_dark"
                            android:textSize="18sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40px"
                            android:layout_marginTop="10dp"
                            android:text="@string/help_language_content"
                            android:textColor="@android:color/primary_text_light"
                            android:textSize="14sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="40px"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_language_1" />

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:layout_marginStart="30px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_language_2" />

                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/fragment_card_margin"
                    app:cardCornerRadius="@dimen/fragment_card_radius"
                    app:cardElevation="@dimen/fragment_card_elevation">

                    <LinearLayout
                        android:id="@+id/ll_power_custom"
                        android:layout_width="match_parent"
                        android:layout_height="350dp"
                        android:layout_marginStart="40px"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="40px"
                            android:text="@string/help_powerkey_title"
                            android:textColor="@android:color/holo_green_dark"
                            android:textSize="18sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="@string/help_powerkey_content"
                            android:textColor="@android:color/primary_text_light"
                            android:textSize="14sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="150dp"
                                android:layout_height="190dp"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_power_1" />

                            <ImageView
                                android:layout_width="150dp"
                                android:layout_height="190dp"
                                android:layout_marginStart="25px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_power_2" />

                            <ImageView
                                android:layout_width="150dp"
                                android:layout_height="190dp"
                                android:layout_marginStart="25px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_power_3" />

                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/fragment_card_margin"
                    app:cardCornerRadius="@dimen/fragment_card_radius"
                    app:cardElevation="@dimen/fragment_card_elevation">

                    <LinearLayout
                        android:id="@+id/ll_trouble"
                        android:layout_width="380dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="40px"
                        android:layout_marginTop="40px"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/help_lighthome_title"
                            android:textColor="@android:color/holo_green_dark"
                            android:textSize="18sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="@string/help_lighthome_content"
                            android:textColor="@android:color/primary_text_light"
                            android:textSize="14sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_weather_1" />

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:layout_marginStart="40px"
                                android:layout_marginEnd="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_weather_2" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:layout_marginBottom="40px"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_weather_3" />

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_weather_4" />

                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/fragment_card_margin"
                    app:cardCornerRadius="@dimen/fragment_card_radius"
                    app:cardElevation="@dimen/fragment_card_elevation">

                    <LinearLayout
                        android:id="@+id/ll_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="40px"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/help_datetime_title"
                            android:textColor="@android:color/holo_green_dark"
                            android:textSize="18sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="10dp"
                            android:text="@string/help_datetime_content"
                            android:textColor="@android:color/primary_text_light"
                            android:textSize="14sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_datetime_1" />

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_datetime_2" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_datetime_3" />

                            <ImageView
                                android:layout_width="250px"
                                android:layout_height="300px"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_datetime_4" />

                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/fragment_card_margin"
                app:cardCornerRadius="@dimen/fragment_card_radius"
                app:cardElevation="@dimen/fragment_card_elevation">

                <LinearLayout
                    android:id="@+id/ll_no_wifi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="25dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/help_ota_title"
                        android:textColor="@android:color/holo_green_dark"
                        android:textSize="18sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="@string/help_ota_content"
                        android:textColor="@android:color/primary_text_light"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="200dp"
                            android:layout_height="350dp"
                            android:scaleType="fitXY"
                            android:src="@mipmap/help_ota_1" />

                        <ImageView
                            android:layout_width="200dp"
                            android:layout_height="350dp"
                            android:layout_marginStart="20dp"
                            android:scaleType="fitXY"
                            android:src="@mipmap/help_ota_2" />

                        <ImageView
                            android:layout_width="200dp"
                            android:layout_height="350dp"
                            android:layout_marginStart="20dp"
                            android:scaleType="fitXY"
                            android:src="@mipmap/help_ota_3" />

                    </LinearLayout>

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
