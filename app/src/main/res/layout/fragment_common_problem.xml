                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:padding="40px"
                    android:orientation="horizontal">

                    <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/fragment_card_margin"
                        app:cardCornerRadius="@dimen/fragment_card_radius"
                        app:cardElevation="@dimen/fragment_card_elevation">

                        <LinearLayout
                            android:layout_width="380dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="40px"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:textColor="@android:color/primary_text_light"
                                android:text="@string/help_firemare_usb_content"
                                android:textSize="14sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@android:color/primary_text_light"
                                android:text="@string/help_firemare_usb_1"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn1" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_2"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_3"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_4"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn3" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_5"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn4" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_6"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn5" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_7"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn6" />

                            <TextView
                                android:id="@+id/tv_burn_8_textDesc"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_8"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:id="@+id/imgBurnPic_GTKing"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_below="@+id/tv_burn_8_textDesc"
                                android:layout_marginTop="20dp"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_burn7" />

                            <ImageView
                                android:id="@+id/imgBurnPic_GTKingPro"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_below="@+id/tv_burn_8_textDesc"
                                android:layout_marginTop="20dp"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_burn7_gtking_pro" />

                            <ImageView
                                android:id="@+id/imgBurnPic_GSKing_X"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_below="@+id/tv_burn_8_textDesc"
                                android:layout_marginTop="20dp"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_burn7_gsking_x" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_9"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn8" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_usb_10"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14dp" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_burn9" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/fragment_card_margin"
                        app:cardCornerRadius="@dimen/fragment_card_radius"
                        app:cardElevation="@dimen/fragment_card_elevation">

                        <LinearLayout
                            android:layout_width="380dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="40px"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@android:color/primary_text_light"
                                android:layout_marginStart="40px"
                                android:text="@string/help_firemare_sd_content"
                                android:textSize="14sp" />

                            <ImageView
                                android:id="@+id/img_sdcard_burn_help_sd1"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:layout_marginTop="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_sd1" />

                            <ImageView
                                android:id="@+id/img_sdcard_burn_help_sd1_big"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:layout_marginTop="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_sd1_big" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="40px"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_sd_1"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14sp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_sd2" />

                            <TextView
                                android:id="@+id/tv_sdcard_burn_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="40px"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_sd_2"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14sp" />

                            <ImageView
                                android:id="@+id/img_sdcard_burn_help_sd3_gtking"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_sd3" />

                            <ImageView
                                android:id="@+id/img_sdcard_burn_help_sd3_gtking_pro"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_below="@+id/tv_sdcard_burn_2"
                                android:layout_marginTop="20dp"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_sd3_gtking_pro" />

                            <ImageView
                                android:id="@+id/img_sdcard_burn_help_sd3_gsking_x"
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_below="@+id/tv_sdcard_burn_2"
                                android:layout_marginTop="20dp"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_sd3_gsking_x" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="40px"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_sd_3"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14sp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="250px"
                                android:layout_marginStart="40px"
                                android:src="@mipmap/help_sd4" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="40px"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_sd_4"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14sp" />

                            <ImageView
                                android:layout_width="400px"
                                android:layout_height="300px"
                                android:layout_marginStart="40px"
                                android:scaleType="fitXY"
                                android:src="@mipmap/help_sd5" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="40px"
                                android:layout_marginTop="40px"
                                android:text="@string/help_firemare_sd_5"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="14sp" />

